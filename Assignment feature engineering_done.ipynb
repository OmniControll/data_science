      "source": [
        "# creating bins for tenure\n",
        "\n",
        "labels = ['0- 10 months', '10 -20 months', '30 -40 months', '40  - 50 months', '50- 60 months'] \n",
        "\n",
        "bins = [0, 10, 20, 30, 40, 50]\n",
        "\n",
        "df_dummies['Tenure_bin'] = pd.cut(df_dummies['tenure'], bins = bins, labels = labels)\n",
        "print(df_dummies['Tenure_bin'])\n",
        "\n",
        "\n",
        "tenure_churn = df_dummies.groupby('Tenure_bin')['Churn_Yes'].mean()\n",
        "print(tenure_churn * 100)"
      ]
